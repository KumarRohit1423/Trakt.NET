{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../Lib"
        }
      ],
      "dest": "obj/api",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "src": "obj/api"
      },
      {
        "files": [
          "Documentation/toc.yml",
          "Documentation/*.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [ "Documentation/images/**" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Trakt.NET",
      "_appFooter": "The MIT License (MIT) | Copyright © 2016 - 2022 Henrik Fröhling et al.",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "../../docs/_site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}