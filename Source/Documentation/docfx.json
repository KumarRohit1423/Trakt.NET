{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../Lib"
        }
      ],
      "dest": "temp/api",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "temp/api",
        "dest": "api",
        "exclude": [ "filterConfig.yml" ]
      },
      {
        "files": [
          "gettingstarted/**/*.{md,yml}",
          "apispec/**/*.{md,yml}",
          "*.{md,yml}"
        ],
        "exclude": [ "filterConfig.yml" ]
      },
      {
        "files": "Changelog.md",
        "src": "../.."
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": "apispec/*.md",
    "dest": "../../docs/_site",
    "globalMetadata": {
      "_appTitle": "Trakt.NET",
      "_appFooter": "<span>The MIT License (MIT)<br />Copyright © 2016 - 2023 Henrik Fröhling et al.<br />Made with <a href=\"https://dotnet.github.io/docfx/\">docfx</a></span>",
      "_enableSearch": true,
      "_gitContribute": {
        "branch": "develop"
      }
    },
    "template": [
      "default"
    ],
    "fileMetadataFiles": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "postProcessors": [ "ExtractSearchIndex" ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}
