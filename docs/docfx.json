{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../Source/Lib"
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "exclude": [ "_site/**", "obj/**", "filterConfig.yml" ]
      },
      {
        "files": "Changelog.md",
        "src": ".."
      }
    ],
    "resource": [
      {
        "files": [ "**/images/**", "codesnippet/**" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Trakt.NET",
      "_appFooter": "<span>The MIT License (MIT)<br />Copyright © 2016 - 2023 Henrik Fröhling et al.<br />Made with <a href='https://dotnet.github.io/docfx/'>docfx</a></span>",
      "_enableSearch": true,
      "_gitContribute": {
        "branch": "develop"
      }
    },
    "template": [
      "default",
      "templates/singulinkfx",
      "plugins/memberpage",
      "plugins/memberpage-extras"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}
